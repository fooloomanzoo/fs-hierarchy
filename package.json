{
  "name": "fs-hierarchy",
  "description": "create a hierarchy map of files and folders",
  "keywords": [
    "hierarchy",
    "tree-structure",
    "tree-map",
    "tree",
    "fs",
    "nodejs",
    "cli"
  ],
  "version": "1.2.2",
  "license": "MIT",
  "type": "module",
  "private": false,
  "author": "Johannes Brautzsch @fooloomanzoo",
  "homepage": "https://github.com/fooloomanzoo/fs-hierarchy",
  "bugs": "https://github.com/fooloomanzoo/fs-hierarchy/issues",
  "repository": "fooloomanzoo/fs-hierarchy",
  "main": "bin/run.js",
  "types": "dist/types.d.ts",
  "files": [
    "/bin",
    "/dist",
    "oclif.manifest.json"
  ],
  "exports": "./dist/index.js",
  "bin": {
    "fs-hierarchy": "./bin/run.js"
  },
  "oclif": {
    "bin": "fs-hierarchy",
    "commands": {
      "strategy": "single",
      "target": "./dist/commands/index.js"
    },
    "default": "hierarchy",
    "dirname": "fs-hierarchy"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "docs": "oclif manifest && oclif readme && tsx utils/readme.ts",
    "lint": "eslint ./src --ext .ts -c .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && npm run docs",
    "prepare": "npm run build",
    "version": "npm run docs && git add ."
  },
  "dependencies": {
    "@oclif/core": "^3.26.4",
    "@oclif/plugin-autocomplete": "^3.0.16",
    "@oclif/plugin-help": "^6.0.21",
    "minimatch": "^9.0.4"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.11",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.1.3",
    "eslint-config-oclif-typescript": "^3.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-node": "~11.1.0",
    "mocha": "^10.4.0",
    "oclif": "^4.9.0",
    "shx": "^0.3.4",
    "tsx": "~4.7.3",
    "typescript": "^5.4.5",
    "typescript-json-schema": "^0.63.0"
  },
  "engines": {
    "node": ">=18"
  }
}
