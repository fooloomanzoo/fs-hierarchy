{
  "name": "fs-hierarchy",
  "description": "create a hierarchy map of files and folders",
  "version": "1.0.2",
  "author": "Johannes Brautzsch @fooloomanzoo",
  "bin": {
    "fs-hierarchy": "./bin/run"
  },
  "bugs": "https://github.com/fooloomanzoo/fs-hierarchy/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "cli-ux": "^5.4.6",
    "glob-to-regexp": "^0.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.10.0",
    "@types/glob-to-regexp": "^0.4.0",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.5",
    "mocha": "^5.2.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "files": [
    "/bin",
    "/lib",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/fooloomanzoo/fs-hierarchy",
  "keywords": [
    "hierarchy",
    "tree-structure",
    "tree-map",
    "tree",
    "fs",
    "nodejs",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "fs-hierarchy",
    "commands": "./src/commands",
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "fooloomanzoo/fs-hierarchy",
  "private": false,
  "scripts": {
    "pretest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && node utils/readme && git add .",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev manifest && node utils/readme && git add ."
  },
  "types": "lib/types.d.ts"
}
